RESOURCE_MANAGER_S3_CONFIG_SECRET_ACCESS_KEY="Nj8TibC7CKa7aywYkIPVfsgiiDkimLACeK3LUXrQ"
RESOURCE_MANAGER_S3_CONFIG_DISABLE_SSL="false"
RESOURCE_MANAGER_S3_CONFIG_FORCE_PATH_STYLE="false"

